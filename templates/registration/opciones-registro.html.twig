        <div class="col-md-3 p-2 mx-auto">
            <a href="{{ path('app_register_carrera') }}" class="w-100 btn btn-lg btn-primary">Registrar Cuenta Orlas</a>
            <br/>
            <br/>
            <a href="{{ path('app_register_social') }}" class="w-100 btn btn-lg btn-primary">Registrar Cuenta Reportajes</a>
        </div>
        <!--End Seccion -->
        <div class="row" style="padding:0; margin:0; height:30vh;">
            <div class="malla"></div>
            <div class="bg-container" style="background: #a8a8a8;">
                <img width="100%" style="min-width: 600px" src="{{ asset('build/img/Inicio.jpg') }}">
            </div>
        </div>
    </div>
</div>

{% endblock %}
{% block javascripts %}
